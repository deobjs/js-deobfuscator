{
  "name": "js-de-obfuscator",
  "version": "0.3.0",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "example",
    "website"
  ],
  "type": "module",
  "scripts": {
    "website:dev": "bun run -F website --elide-lines 0 dev",
    "website:build": "bun run -F website --elide-lines 0 build",
    "website:generate": "bun run -F website --elide-lines 0 generate",
    "website:serve": "bun run -F website --elide-lines 0 serve",
    "lint": "eslint './{packages,website,scripts,meta}/**/*.{js,ts,tsx,vue,md,json}'",
    "format": "bunx oxfmt",
    "release": "bumpp -r",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.2",
    "bumpp": "^9.2.0",
    "deob": "workspace:*",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  }
}
